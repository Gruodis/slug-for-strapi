{
  "name": "@gruodis/slug-for-strapi",
  "version": "1.2.2",
  "description": "Does what it says. Slug for Strapi.",
  "source": "./src/server/index.ts",
  "main": "./dist/server/index.js",
  "module": "./dist/server/index.mjs",
  "types": "./dist/server/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "develop": "pack-up watch",
    "build": "pack-up build",
    "test": "jest"
  },
  "exports": {
    "./strapi-server": {
      "source": "./src/server/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    },
    "./strapi-admin": {
      "source": "./src/admin/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "strapi",
    "plugin",
    "slug",
    "auto-generation",
    "universal"
  ],
  "author": "Gruodis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gruodis/slug-for-strapi.git"
  },
  "bugs": {
    "url": "https://github.com/Gruodis/slug-for-strapi/issues"
  },
  "homepage": "https://github.com/Gruodis/slug-for-strapi#readme",
  "strapi": {
    "name": "slug-for-strapi",
    "displayName": "Slug For Strapi",
    "description": "Does what it says. Slug for Strapi.",
    "kind": "plugin"
  },
  "dependencies": {
    "slugify": "^1.6.6"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "dist",
    "strapi-admin.js",
    "strapi-server.js",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@strapi/pack-up": "^5.1.1",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6"
  }
}
